function SprPr1(){
	if(addChild(spr_CNA)){
		m_A1.mouseEnabled=false;
		m_A2.mouseEnabled=false;
		m_A3.mouseEnabled=false;
		m_A4.mouseEnabled=false;
		m_A5.mouseEnabled=false;
		m_A6.mouseEnabled=false;
		m_A7.mouseEnabled=false;
		m_A8.mouseEnabled=false;
		m_A9.mouseEnabled=false;
		m_A10.mouseEnabled=false;
		m_A11.mouseEnabled=false;
		m_A12.mouseEnabled=false}
}
function SprPr2(){
	if(addChild(spr_CA)){
		m_A1.mouseEnabled=true;
		m_A2.mouseEnabled=true;
		m_A3.mouseEnabled=true;
		m_A4.mouseEnabled=true;
		m_A5.mouseEnabled=true;
		m_A6.mouseEnabled=true;
		m_A7.mouseEnabled=true;
		m_A8.mouseEnabled=true;
		m_A9.mouseEnabled=true;
		m_A10.mouseEnabled=true;
		m_A11.mouseEnabled=true;
		m_A12.mouseEnabled=true}
}

spr_CA.addEventListener(MouseEvent.CLICK, clickSPR);
	function clickSPR(event:MouseEvent):void {
		removeChild(spr_CA);
		addChild(spr);
		addChild(spr_ex);
		addChild(spr_CNA);
		SprPr1();
}

spr_ex.addEventListener(MouseEvent.CLICK, clickSPRex);
	function clickSPRex(event:MouseEvent):void {
		if(addChild(spr_CNA)){removeChild(spr_CNA)}
		removeChild(spr);
		removeChild(spr_ex);
		addChild(spr_CA);
		SprPr2();
}



