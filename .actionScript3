//Задание №1 Цикада

function t1_Test1(){
	if(t1_test1==1){
	removeChild(t1_num648r);
	addChild(t1_num648);
	t1_test1=0}
}
function t1_Test2(){
	if(t1_test2==1){
	removeChild(t1_num24r);
	addChild(t1_num24);
	t1_test2=0}
}
function t1_Test3(){
	if(t1_test3==1){
	removeChild(t1_num19100r);
	addChild(t1_num19100);
	t1_test3=0}
}

function t1_Victory(){
	if (t1_1==true && t1_2==true && t1_3==true) {
		proverka1=true;
		addChild(t1_Vic);
		t1_Vic.gotoAndPlay(1);}
}

t1_num83.addEventListener(MouseEvent.CLICK, clickNum83);
function clickNum83(evt:MouseEvent): void {
	t1_1 = true;t1_Victory();
	t1_Test1();
	t1_num648.mouseEnabled = false;
	removeChild(t1_num83);
	addChild(t1_num83g);
}

t1_num648.addEventListener(MouseEvent.CLICK, clickNum648);
function clickNum648(evt:MouseEvent): void {
	removeChild(t1_num648);
	addChild(t1_num648r);
	t1_test1=1;
}

t1_num4.addEventListener(MouseEvent.CLICK, clickNum4);
function clickNum4(evt:MouseEvent): void {
	t1_2 = true;t1_Victory();
	t1_Test2();
	t1_num24.mouseEnabled = false;
	removeChild(t1_num4);
	addChild(t1_num4g);
} 

t1_num24.addEventListener(MouseEvent.CLICK, clickNum24);
function clickNum24(evt:MouseEvent): void {
	removeChild(t1_num24);
	addChild(t1_num24r);
	t1_test2=1;
}

t1_num19100.addEventListener(MouseEvent.CLICK, clickNum19100);
function clickNum19100(evt:MouseEvent): void {
	removeChild(t1_num19100);
	addChild(t1_num19100r);
	t1_test3=1;
}

t1_num1000.addEventListener(MouseEvent.CLICK, clickNum1000);
function clickNum1000(evt:MouseEvent): void {
	t1_Test3();
	t1_3 = true;t1_Victory();
	t1_num19100.mouseEnabled = false;
	removeChild(t1_num1000);
	addChild(t1_num1000g);
}

//Переход в меню

t1_Vic.addEventListener(MouseEvent.CLICK, clickT1vc);
function clickT1vc(event:MouseEvent):void 
{
if(proverka1==true){
	removeChild(t1_Vic); 
