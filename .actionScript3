//Задание №2 Контрольные станции

function Vic(){
	if (vic==0) {
		proverka2=true;
		addChild(ks_vic)};
}

function Num(){
	if (num==6) {addChild(ks_Num6); removeChild(ks_Num7)}
	else if (num==5) {addChild(ks_Num5); removeChild(ks_Num6)}
	else if (num==4) {addChild(ks_Num4); removeChild(ks_Num5)}
	else if (num==3) {addChild(ks_Num3); removeChild(ks_Num4)}
	else if (num==2) {addChild(ks_Num2); removeChild(ks_Num3)}
	else if (num==1) {addChild(ks_Num1); removeChild(ks_Num2)}
	else if (num==0) {addChild(ks_Num0); removeChild(ks_Num1)};
}

