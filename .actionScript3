//Конец игры

te_cont.addEventListener(MouseEvent.CLICK, tect);
	function tect(event:MouseEvent):void {
		addChild(fon_te);
		addChild(but_te);
		removeChild(te_cont);
}

but_te.addEventListener(MouseEvent.CLICK, bt);
	function bt(event:MouseEvent):void {
		gotoAndPlay("The End","Final");
}
